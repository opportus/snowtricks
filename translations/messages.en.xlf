                <source>user.layout.link.title.sign_in</source>
                <target>Sign In</target>
            </trans-unit>
            <trans-unit id="user.layout.link.anchor.sign_out">
                <source>user.layout.link.anchor.sign_out</source>
                <target>Sign Out</target>
            </trans-unit>
            <trans-unit id="user.layout.link.title.sign_out">
                <source>user.layout.link.title.sign_out</source>
                <target>Sign Out</target>
            </trans-unit>

